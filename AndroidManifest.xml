<?xml version="1.0" encoding="utf-8"?>
<manifest
    android:compileSdkVersion="34"
    android:compileSdkVersionCodename="14"
    package="com.random.chat.app"
    platformBuildVersionCode="34"
    platformBuildVersionName="14"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />
    <!-- Enquanto está sendo usado, este app pode tirar fotos e gravar vídeos usando a câmera. -->
    <uses-permission
        android:name="android.permission.CAMERA" />
    <!-- Permite que o app crie soquetes de rede e utilize protocolos de rede personalizados. O navegador e outros apps fornecem meios de enviar dados para a Internet, e por isso esta permissão não é necessária para enviar os dados. -->
    <uses-permission
        android:name="android.permission.INTERNET" />
    <!-- Permite que o app leia arquivos de imagem do armazenamento compartilhado. -->
    <uses-permission
        android:name="android.permission.READ_MEDIA_IMAGES" />
    <!-- Permite que o app leia arquivos de áudio do armazenamento compartilhado. -->
    <uses-permission
        android:name="android.permission.READ_MEDIA_AUDIO" />
    <!-- Permite que o app leia arquivos de vídeo do armazenamento compartilhado. -->
    <uses-permission
        android:name="android.permission.READ_MEDIA_VIDEO" />
    <!-- Permite que o app leia o conteúdo do armaz. compartilhado. -->
    <uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="32" />
    <!-- Permite que o app grave o conteúdo do armaz. compartilhado. -->
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="29" />
    <!-- Permite que o app acesse informações sobre conexões de rede, como as redes existentes e conectadas. -->
    <uses-permission
        android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- Permite que o app controle a vibração. -->
    <uses-permission
        android:name="android.permission.VIBRATE" />
    <!-- Enquanto está sendo usado, este app pode gravar áudio usando o microfone. -->
    <uses-permission
        android:name="android.permission.RECORD_AUDIO" />
    <!-- Permite que o app impeça a suspensão do telefone. -->
    <uses-permission
        android:name="android.permission.WAKE_LOCK" />
    <!-- Permite que o usuário compre itens no Google Play usando este app. -->
    <uses-permission
        android:name="com.android.vending.BILLING" />
    <!-- Permite que um app editor acesse um ID de publicidade válido direta ou indiretamente. -->
    <uses-permission
        android:name="com.google.android.gms.permission.AD_ID" />
    <!-- Permite que o app mostre notificações -->
    <uses-permission
        android:name="android.permission.POST_NOTIFICATIONS" />
    <!-- Permite que um aplicativo tenha acesso à API AdId. -->
    <uses-permission
        android:name="android.permission.ACCESS_ADSERVICES_AD_ID" />
    <queries>
        <intent>
            <action
                android:name="android.media.action.IMAGE_CAPTURE" />
        </intent>
        <intent>
            <action
                android:name="android.intent.action.OPEN_DOCUMENT_TREE" />
        </intent>
        <package
            android:name="com.facebook.katana" />
        <intent>
            <action
                android:name="com.android.vending.billing.InAppBillingService.BIND" />
        </intent>
        <intent>
            <action
                android:name="android.intent.action.VIEW" />
            <category
                android:name="android.intent.category.BROWSABLE" />
            <data
                android:scheme="https" />
        </intent>
        <intent>
            <action
                android:name="android.support.customtabs.action.CustomTabsService" />
        </intent>
        <intent>
            <action
                android:name="com.applovin.am.intent.action.APPHUB_SERVICE" />
        </intent>
